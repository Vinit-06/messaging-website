{
  "components": [
    {
      "name": "ChatMessage",
      "displayName": "Chat Message",
      "description": "A chat message bubble component",
      "category": "Messaging",
      "inputs": [
        {
          "name": "message",
          "type": "object",
          "required": true,
          "subFields": [
            {
              "name": "content",
              "type": "string",
              "required": true,
              "helperText": "The message text content"
            },
            {
              "name": "senderId",
              "type": "string",
              "required": true,
              "helperText": "ID of the message sender"
            },
            {
              "name": "senderName",
              "type": "string",
              "required": true,
              "helperText": "Name of the message sender"
            },
            {
              "name": "timestamp",
              "type": "date",
              "required": true,
              "helperText": "When the message was sent"
            },
            {
              "name": "type",
              "type": "string",
              "enum": ["text", "file", "image"],
              "defaultValue": "text",
              "helperText": "Type of message content"
            },
            {
              "name": "status",
              "type": "string",
              "enum": ["sending", "sent", "delivered", "read", "failed"],
              "defaultValue": "sent",
              "helperText": "Message delivery status"
            }
          ]
        },
        {
          "name": "isOwn",
          "type": "boolean",
          "defaultValue": false,
          "helperText": "Whether this message is from the current user"
        },
        {
          "name": "showStatus",
          "type": "boolean",
          "defaultValue": false,
          "helperText": "Whether to show message status indicator"
        }
      ],
      "defaultChildren": []
    },
    {
      "name": "ChatList",
      "displayName": "Chat List",
      "description": "A list of chat conversations",
      "category": "Messaging",
      "inputs": [
        {
          "name": "chats",
          "type": "list",
          "required": true,
          "subFields": [
            {
              "name": "id",
              "type": "string",
              "required": true
            },
            {
              "name": "name",
              "type": "string",
              "required": true
            },
            {
              "name": "avatar",
              "type": "file",
              "allowedFileTypes": ["jpeg", "jpg", "png", "svg", "webp"]
            },
            {
              "name": "lastMessage",
              "type": "string"
            },
            {
              "name": "lastMessageTime",
              "type": "date"
            },
            {
              "name": "unreadCount",
              "type": "number",
              "defaultValue": 0
            },
            {
              "name": "type",
              "type": "string",
              "enum": ["direct", "group"],
              "defaultValue": "direct"
            },
            {
              "name": "isOnline",
              "type": "boolean",
              "defaultValue": false
            }
          ]
        },
        {
          "name": "collapsed",
          "type": "boolean",
          "defaultValue": false,
          "helperText": "Whether the chat list is in collapsed mode"
        },
        {
          "name": "onChatSelect",
          "type": "action",
          "helperText": "Action to perform when a chat is selected"
        }
      ],
      "defaultChildren": []
    },
    {
      "name": "ChatWindow",
      "displayName": "Chat Window",
      "description": "The main chat interface with messages and input",
      "category": "Messaging",
      "inputs": [
        {
          "name": "chat",
          "type": "object",
          "required": true,
          "subFields": [
            {
              "name": "id",
              "type": "string",
              "required": true
            },
            {
              "name": "name",
              "type": "string",
              "required": true
            },
            {
              "name": "avatar",
              "type": "file",
              "allowedFileTypes": ["jpeg", "jpg", "png", "svg", "webp"]
            },
            {
              "name": "type",
              "type": "string",
              "enum": ["direct", "group"],
              "defaultValue": "direct"
            },
            {
              "name": "isOnline",
              "type": "boolean",
              "defaultValue": false
            },
            {
              "name": "memberCount",
              "type": "number"
            }
          ]
        },
        {
          "name": "messages",
          "type": "list",
          "defaultValue": [],
          "subFields": [
            {
              "name": "id",
              "type": "string",
              "required": true
            },
            {
              "name": "content",
              "type": "string",
              "required": true
            },
            {
              "name": "senderId",
              "type": "string",
              "required": true
            },
            {
              "name": "timestamp",
              "type": "date",
              "required": true
            },
            {
              "name": "type",
              "type": "string",
              "enum": ["text", "file", "image"],
              "defaultValue": "text"
            }
          ]
        },
        {
          "name": "onSendMessage",
          "type": "action",
          "helperText": "Action to perform when sending a message"
        }
      ],
      "defaultChildren": []
    },
    {
      "name": "UserProfile",
      "displayName": "User Profile",
      "description": "User profile modal component",
      "category": "User Interface",
      "inputs": [
        {
          "name": "user",
          "type": "object",
          "required": true,
          "subFields": [
            {
              "name": "id",
              "type": "string",
              "required": true
            },
            {
              "name": "email",
              "type": "string",
              "required": true
            },
            {
              "name": "user_metadata",
              "type": "object",
              "subFields": [
                {
                  "name": "full_name",
                  "type": "string"
                },
                {
                  "name": "avatar_url",
                  "type": "file",
                  "allowedFileTypes": ["jpeg", "jpg", "png", "svg", "webp"]
                },
                {
                  "name": "bio",
                  "type": "longText"
                },
                {
                  "name": "phone",
                  "type": "string"
                },
                {
                  "name": "location",
                  "type": "string"
                },
                {
                  "name": "website",
                  "type": "url"
                }
              ]
            },
            {
              "name": "created_at",
              "type": "date"
            }
          ]
        },
        {
          "name": "onClose",
          "type": "action",
          "helperText": "Action to perform when closing the profile"
        },
        {
          "name": "onUpdate",
          "type": "action",
          "helperText": "Action to perform when updating profile"
        }
      ],
      "defaultChildren": []
    },
    {
      "name": "AIFeaturesPanel",
      "displayName": "AI Features Panel",
      "description": "AI-powered features for chat enhancement",
      "category": "AI",
      "inputs": [
        {
          "name": "activeChat",
          "type": "object",
          "subFields": [
            {
              "name": "id",
              "type": "string"
            },
            {
              "name": "name",
              "type": "string"
            }
          ]
        },
        {
          "name": "features",
          "type": "list",
          "defaultValue": ["chat", "translate", "summarize", "suggestions"],
          "subFields": [
            {
              "name": "feature",
              "type": "string",
              "enum": ["chat", "translate", "summarize", "suggestions"]
            }
          ],
          "helperText": "Available AI features to enable"
        },
        {
          "name": "onClose",
          "type": "action",
          "helperText": "Action to perform when closing the AI panel"
        }
      ],
      "defaultChildren": []
    },
    {
