      "description": "Full chat application layout with sidebar, chat list, and messaging",
      "category": "Layouts",
      "model": "page",
      "inputs": [
        {
          "name": "user",
          "type": "object",
          "required": true,
          "helperText": "Current authenticated user object"
        },
        {
          "name": "initialChats",
          "type": "list",
          "defaultValue": [],
          "helperText": "Initial list of chat conversations"
        },
        {
          "name": "features",
          "type": "object",
          "subFields": [
            {
              "name": "enableAI",
              "type": "boolean",
              "defaultValue": true
            },
            {
              "name": "enableFileUpload",
              "type": "boolean",
              "defaultValue": true
            },
            {
              "name": "enableGroupChat",
              "type": "boolean",
              "defaultValue": true
            },
            {
              "name": "enableVoiceChat",
              "type": "boolean",
              "defaultValue": false
            },
            {
              "name": "enableVideoChat",
              "type": "boolean",
              "defaultValue": false
            }
          ],
          "helperText": "Feature toggles for the chat dashboard"
        }
      ]
    }
  ],
  "dataModels": [
    {
      "name": "User",
      "fields": [
        {
          "name": "id",
          "type": "string",
