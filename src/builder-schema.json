          "defaultValue": false
        },
        {
          "name": "timestamp",
          "type": "date",
          "required": true
        }
      ]
    }
  ],
  "integrations": {
    "supabase": {
      "authTables": ["users"],
      "realtimeEnabled": true,
      "rls": true
    },
    "vercelAI": {
      "models": ["gpt-3.5-turbo", "gpt-4"],
      "features": ["chat", "completion", "streaming"]
    },
    "websocket": {
      "events": ["message", "typing", "user-status", "chat-update"],
      "rooms": true
    }
  },
